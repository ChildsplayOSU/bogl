game TypeError

type Board = Grid(1,1) of Nothing
type Input = Nothing

typeError : Int -> Bool
typeError(x) = x == True

typeError2 : Int -> Bool
typeError2(x) = x

notTypeError3 : Int -> Int | Option1 | Option2
notTypeError3(x) = if x == 0 then Option1 else if x == 1 then Option2 else x

typeError3 : Int -> Int
typeError3(x) = notTypeError3(x) + 2
